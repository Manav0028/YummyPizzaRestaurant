create table menu (
    product_id int primary key,
productName varchar(50),
productPrice double,
productType varchar(50), -- // enum
calories double,
pizzaBase varchar(50),-- // enum
pizzaSize varchar(50), -- // enum
 pizzaSauce varchar(50) -- // enum
)